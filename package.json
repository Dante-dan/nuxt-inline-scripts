{
  "name": "nuxt-inline-scripts",
  "version": "1.0.11",
  "description": "Enable/Disable inlineScripts for Nuxt3",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "prepare": "nuxi prepare",
    "prepack": "nuxi prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dante-dan/nuxt-inlineScripts.git"
  },
  "license": "MIT",
  "author": {
    "name": "Dante-dan",
    "email": "duanjl.chian@gmail.com",
    "url": "https://github.com/Dante-dan"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "nuxt-tool",
    "nuxt3"
  ],
  "bugs": {
    "url": "https://github.com/Dante-dan/nuxt-inlineScripts/issues"
  },
  "homepage": "https://github.com/Dante-dan/nuxt-inlineScripts#readme",
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "defu": "^6.1.4",
    "nitropack": "^2.0.0",
    "ohash": "^1.0.0",
    "pathe": "^1.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.14",
    "@nuxt/eslint-config": "^0.5.2",
    "@nuxt/module-builder": "^0.8.3",
    "@nuxt/schema": "^3.13.0",
    "@nuxt/test-utils": "^3.14.1",
    "@types/node": "^22.5.0",
    "changelogen": "^0.5.5",
    "eslint": "^9.9.1",
    "nuxt": "^3.0.0",
    "prettier": "^3.3.3",
    "typescript": "latest",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "prettier --write --ignore-unknown"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "build",
                "section": "Dependencies and Other Build Updates",
                "hidden": false
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}